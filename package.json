{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "ngx-view-only",
  "version": "1.0.4",
  "ngPackage": {
    "lib": {
      "entryFile": "app/index.ts"
    }
  },
  "description": "Angular version >= 2 module. Render only visible elements of a list.",
  "main": "dist/bundles/ngx-view-only.umd.js",
  "scripts": {
    "clean_modules": "rm -r node_modules",
    "pack_module": "ng-packagr -p package.json",
    "build": "npm i && npm run pack_module && npm run clean_modules"
  },
  "keywords": [
    "Angular",
    "Angular 2",
    "Angular 3",
    "Angular 4",
    "Angular 5",
    "Angular 6",
    "Angular x",
    "ngx",
    "ng2",
    "ng",
    "module",
    "renderer",
    "render",
    "view",
    "list",
    "infinite",
    "scroll"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GasparErrobidart/ngx-view-only"
  },
  "author": "Gaspar Gabriel Errobidart",
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "6.0.3",
    "@angular/core": "6.0.3",
    "rxjs": "^6.2.0",
    "zone.js": "^0.8.26",
    "@angular/compiler": "6.0.3",
    "@angular/compiler-cli": "6.0.3",
    "ng-packagr": "3.0.1",
    "typescript": "2.7.2"
  }
}
